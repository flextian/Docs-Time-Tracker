{
	"manifest_version": 2,
	"name": "Google Docs Time Tracker",
	"version": "0.1",
	"permissions": ["tabs"],
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
	"content_scripts": [
		{
			"matches": ["https://docs.google.com/document/d*"],
			"js": ["jquery.js", "content.js"]
		}
	],
	"background": {
		"scripts": ["background.js"]
	}
}